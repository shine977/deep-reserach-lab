{
  "name": "deep-research-lab",
  "version": "0.1.0",
  "description": "TypeScript implementation of DeepSearch system with plugin architecture",
  "private": true,
  "main": "dist/main.js",
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "example": "ts-node -r tsconfig-paths/register src/examples/simple-example.ts",
    "execution-example": "ts-node -r tsconfig-paths/register src/examples/execution-example.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "axios": "^1.6.0",
    "jsonschema": "^1.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "deep-search",
    "typescript",
    "plugin-system",
    "workflow-engine",
    "rxjs"
  ],
  "author": "",
  "license": "MIT"
}